{
  "name": "Shoseki Calculator",
  "description": "Shoseki Estimate Calculator for firefox!",
  "version": "0.0.3",
  "manifest_version": 2,
  "content_scripts": [{
    
    "js": ["content.js"],
	"css": ["estimate.css"],
    "matches": ["http://shosekiranking.blog.fc2.com/*"]
  }],
  "permissions": ["storage", "activeTab"],

  "icons": {
    "16": "/images/shoseki16.png",
    "32": "/images/shoseki32.png",
    "48": "/images/shoseki48.png",
    "128": "/images/shoseki128.png"
  }
}